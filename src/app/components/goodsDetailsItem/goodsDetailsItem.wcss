.item{
    width: 100%;
    height: 100%;
    border-bottom: 1px solid #e5e5e5;
    display: flex;
    align-items: center;
}
.title{
    font-size:26px;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(136,136,136,1);
    line-height:37px;
    margin-left: 5px;
}
.content{
    font-size:26px;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(0,0,0,1);
    line-height:37px;
    margin-left: 20px;
    flex: 1 0 0;
}
.icon{
    margin-right: 30px;
}
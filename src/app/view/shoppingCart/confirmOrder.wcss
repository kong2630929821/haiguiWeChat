.page{
    overflow-x: hidden;
    overflow-y: auto;
    background: #F9F9F9;
    padding-bottom: 120px;
}
.row{
    display: flex;
    align-items: center;
    background: #fff;
}
.row-addr{
    justify-content: space-between;
    height: 88px;
    padding: 0 30px;
}
.row2{
    flex: 1 0 0;
    display: flex;
    justify-content: space-between;
    align-items: baseline;
    background: #fff;
}
.goodsList{
    margin-bottom: 30px;
}
.goodsImg{
    width: 140px;
    height: 140px;
    margin-right: 20px;
    flex-shrink: 0;
}
.column{
    overflow: hidden;
    font-size: 24px;
    color:#888;
    flex: 1 0 0;
}
.goodsTitle{
    color:#222;
    margin-bottom: 10px;
}
.top{
    border-top: 1px solid #E5E5E5;
    margin-bottom: 20px;
    background: #fff;
}
.text{
    height:48px;
    font-size:34px;
    color:rgba(0,0,0,1);
    line-height:48px;
}
.row1{
    border-bottom: 1px solid #E5E5E5;
    padding: 0 30px;
    height:88px;
    line-height:88px;
    position:relative;
    font-size: 26px;
    justify-content: space-between;
}
.total{
    font-size:20px;
    color:#888;
    position: absolute;
    right: 30px;
    height: 88px;
    line-height: 88px;
}
.bottom{
    font-size:20px;
    color:#353535;
    position: fixed;
    bottom: 0;
    text-align: right;
    background: #fff;
    width:100%;
    height: 98px;
}
.pay{
    width:240px;
    height:98px;
    background:#8A4AF3;
    font-size:32px;
    font-weight:500;
    color:rgba(255,255,255,1);
    line-height:98px;
    margin-left: 30px;
    text-align: center;
    display: inline-block;
}
.good-price{
    display: flex;
    align-items: baseline;
    margin-top: 8px;
}
.buy-price{
    font-size:32px;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(138,74,243,1);
    line-height:45px;
}
.original-price{
    margin-left: 10px;
    font-size:20px;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(136,136,136,1);
    line-height:28px;
    text-decoration: line-through;
}
.new-page{
    z-index: 10;
    overflow-x: hidden;
    overflow-y: auto;
    scroll-behavior: smooth;
    -webkit-overflow-scrolling: touch;
    background: #F9F9F9;
    padding-bottom:120px; 
}
.btn{
    height:98px;
    background:rgba(138,74,243,1);
    font-size:32px;
    font-weight:500;
    color:rgba(255,255,255,1);
    line-height:98px;
    position: fixed;
    bottom: 0;
    width: 100%;
    text-align: center;
}
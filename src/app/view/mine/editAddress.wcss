.row{
    display: flex;
    margin-left: 30px;
    align-items: center;
    min-height: 88px;
    font-size: 34px;
}
.row1{
    margin-left: 20px;
    border-bottom: 1px solid #e5e5e5;
    padding: 20px 0;
}
.btn{
    height:98px;
    background:rgba(138,74,243,1);
    border-radius:10px;
    border:2px solid rgba(5,5,5,0.08);
    margin: 40px;
    line-height: 98px;
    font-size:36px;
    color:rgba(255,255,255,1);
    text-align: center;
}
.btn-box{
    position: absolute;
    bottom: 200px;
    left: 0;
    right: 0;
}
.btn1{
    border:2px solid #ccc;
    color: #ccc;
    background: #fff;
}
.column{
    flex-shrink: 0;
    width: 200px;
}
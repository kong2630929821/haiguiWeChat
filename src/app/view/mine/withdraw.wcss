.content{
    margin: 160px 100px;
}
.money{
    height:40px;
    font-size:28px;
    color:rgba(0,0,0,1);
    line-height:40px;
    margin-bottom: 20px;
}
.desc{
    height:28px;
    font-size:20px;
    color:rgba(136,136,136,1);
    line-height:28px;
    margin-top: 20px;
}
.total{
    height:33px;
    font-size:24px;
    color:rgba(0,0,0,1);

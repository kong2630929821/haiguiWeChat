}
.btn{
    margin: 20px 40px;
    height:94px;
    background:rgba(138,74,243,1);
    border-radius:10px;
    border:2px solid rgba(5,5,5,0.08);
    font-size:36px;
    color:rgba(255,255,255,1);
    line-height:94px;
    text-align: center;
}